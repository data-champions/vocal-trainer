{
  "name": "vocal-trainer-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "open http://localhost:8501",
    "dev": "next dev -p 8501 --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "show_network_ip": "sh -c \"ip route get 1 | awk '{print $7}' | head -n 1 | sed 's/$/:8501/'| sed -E 's/ via 192.[0-9.]+//' \""
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "next": "14.2.12",
    "pitchy": "^4.1.0",
    "react": "18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "18.3.1",
    "tone": "^15.1.22",
    "uplot": "^1.6.32"
  },
  "devDependencies": {
    "@types/node": "22.7.4",
    "@types/react": "18.3.9",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.12",
    "husky": "^9.1.7",
    "typescript": "5.6.3"
  }
}
